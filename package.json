{
  "name": "nhsuk-prototype-kit",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/nhsuk/nhsuk-prototype-kit.git"
  },
  "scripts": {
    "start": "node start.js",
    "debug": "node --inspect server.js",
    "setup": "npm install && npm start",
    "lint": "standard '*.js' 'app/**/*.js' 'app/**/*.html' 'assets/scripts/**/*.js'",
    "test": "npm run lint",
    "test-pa11y": "node tests/pa11y/pa11y.js"
  },
  "engines": {
    "node": "8.4.0",
    "npm": "5.5.1"
  },
  "standard": {
    "plugins": [
      "html"
    ],
    "ignore": [
      "assets/javascripts/*.js",
      "public/javascripts/*.js"
    ]
  },
  "dependencies": {
    "basic-auth": "^1.1.0",
    "body-parser": "~1.17.1",
    "browser-sync": "^2.23.6",
    "chalk": "^2.3.2",
    "cheerio": "^1.0.0-rc.2",
    "consolidate": "^0.14.5",
    "cookie-parser": "~1.4.3",
    "cross-spawn": "^5.1.0",
    "cssnano": "^3.10.0",
    "cssnext": "^1.8.4",
    "debug": "^2.6.9",
    "dotenv": "^4.0.0",
    "eslint-plugin-html": "^3.2.1",
    "express": "^4.16.3",
    "express-handlebars": "^3.0.0",
    "express-hbs": "^1.0.4",
    "express-nunjucks": "^2.2.3",
    "express-session": "^1.15.5",
    "express-useragent": "^1.0.12",
    "express-validator": "^4.3.0",
    "frontend-library": "git://github.com/nhsuk/frontend-library.git#0.6.0",
    "gulp": "3.9.1",
    "gulp-nodemon": "^2.2.1",
    "gulp-postcss": "^7.0.1",
    "gulp-sass": "^3.2.1",
    "gulp-sourcemaps": "^2.6.4",
    "hbs": "^4.0.1",
    "marked": "^0.3.19",
    "minimist": "^1.2.0",
    "morgan": "~1.8.1",
    "notifications-node-client": "^4.1.0",
    "nunjucks": "^3.1.2",
    "nunjucks-date-filter": "^0.1.1",
    "onfido-sdk-ui": "^2.2.0",
    "pa11y": "^5.0.3",
    "pa11y-reporter-html": "^1.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-reporter": "^5.0.0",
    "postcss-scss": "^1.0.5",
    "prompt": "^1.0.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.2",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.2.1",
    "serve-favicon": "^2.5.0",
    "standard": "^10.0.3",
    "stylelint": "^8.4.0"
